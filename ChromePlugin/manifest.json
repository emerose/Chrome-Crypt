{
	"name": "ChromeCrypt",
	"version": "0.0.1",
	"description": "Crypto Plugin for Chrome",
	
	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"js": ["jquery-1.4.min.js"]
	}],	
	
	"plugins": [{ 
		"path": "ChromeCrypt.bundle" 
	}],

	"background_page": "background.html",
	
	"permissions": ["tabs", "http://*/*", "https://*/*"]
}
