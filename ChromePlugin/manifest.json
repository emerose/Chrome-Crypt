{
	"name": "ChromeCrypt",
	"version": "0.0.3",
	"description": "Crypto Plugin for Chrome",

  "browser_action": { 
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
	
	"plugins": [ 
    { "path": "ChromeCrypt.plugin", "public": true }
	],

	"background_page": "background.html",
	
	"permissions": ["tabs", "http://*/*", "https://*/*"]
}
